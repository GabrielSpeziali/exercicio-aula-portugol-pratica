programa {
  funcao inicio() {
    cadeia cidadeMora, cidadeEstuda

    escreva("Digite o nome da cidade em que vc mora: ")
    leia(cidadeMora)

    escreva("Digite o nome da cidade em que vc estuda: ")
    leia(cidadeEstuda)

    escreva("A cidade que vc mora é " + cidadeMora + " e a cidade que vc estudo é " + cidadeEstuda)
  }
}
